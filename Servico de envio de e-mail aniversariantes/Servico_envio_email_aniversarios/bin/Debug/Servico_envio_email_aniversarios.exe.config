            <setting name="Select_Aniversariantes" serializeAs="String">
                <value>        select LTRIM(vi.NUM_RGTRO_EMPRG, 0) as num_rgtro_emprg
                , vi.NOME_ABREV as nom_emprg
                , vi.cod_email_emprg
                , vi.nom_area as area
                from OWN_PORTAL.RH_VIW_EMAIL_ANIVER vi
                where lower(vi.cod_email_emprg) like '%funcesp.com.br'
                and vi.NUM_RGTRO_EMPRG = 2565 /* empregados cedidos "502 -   CARLOS ALBERTO DE SOUZA OLIVEIRA"; "1612 - CARLOS FELIPE FRANCA" */
                and vi.aniver != to_char(sysdate, 'DD/MM')
              </value>
            </setting>
            <setting name="Select_Destinatarios" serializeAs="String">
                <value>        select LTRIM(vi.NUM_RGTRO_EMPRG, 0) as num_rgtro_emprg
                , vi.NOME_ABREV as nom_emprg
                , vi.cod_email_emprg
                , vi.nom_area as area
                from OWN_PORTAL.RH_VIW_EMAIL_ANIVER vi
                where lower(vi.cod_email_emprg) like '%funcesp.com.br'
                and vi.NUM_RGTRO_EMPRG = 2565 /* empregados cedidos "502 -   CARLOS ALBERTO DE SOUZA OLIVEIRA"; "1612 - CARLOS FELIPE FRANCA" */
                and vi.aniver != to_char(sysdate, 'DD/MM')
